package kr.co.green;

import java.io.*;
import java.util.*;

import java.util.Scanner;

public class Main2 {
 
    public static void main(String[] args) throws IOException {
        // TODO Auto-generated method stub
        int[][] arr = new int[5][5];
        
        arr[0][0] = 1;
        arr[0][2] = 1;
        arr[0][4] = 1;
        
        for(int i=0;i<5;i++)
        {
        	for(int j=0;j<5;j++)
        	{
        		if(i==0)
        		{
        			System.out.print(arr[i][j]+" ");
        			continue;
        		}
        		else if(j==0)
        		{
        			arr[i][j] = arr[i-1][j+1];
        		}
        		else if(j==4)
        		{
        			arr[i][j] = arr[i-1][j-1];
        		}
        		else
        		{
        			arr[i][j] = arr[i-1][j-1]+arr[i-1][j+1];
        		}
        		System.out.print(arr[i][j]+" ");
        		
        	}
        	System.out.print("\n");
        }
        
        
        
    }
 
}