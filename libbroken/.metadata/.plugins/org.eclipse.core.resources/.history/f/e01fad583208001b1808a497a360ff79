package kr.co.green;


import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner input = new Scanner(System.in);
		int cn = input.nextInt();
		int result = cn;
		for(int i=0;i<cn;i++)
		{
			int num = input.nextInt();
			for(int j=2;j<num;j++)
			{
				if(num%j == 0)
				{
					result--;
					break;
				}
			}
		}
		System.out.println(result);
		
	}
	
	
	

}

