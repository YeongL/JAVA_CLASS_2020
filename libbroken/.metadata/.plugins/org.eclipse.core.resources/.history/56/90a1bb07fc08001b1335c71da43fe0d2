package kr.co.green;


import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner input = new Scanner(System.in);
		int N = input.nextInt();
		input.close();
		int result = fac(N);
		System.out.println(result);
		
	}
	
	public static int fac(int n)
	{
		if(n==0)
		{
			return 1;
		}
		
		return fac(n-1)*n;
	}
	
	
	
	

}

