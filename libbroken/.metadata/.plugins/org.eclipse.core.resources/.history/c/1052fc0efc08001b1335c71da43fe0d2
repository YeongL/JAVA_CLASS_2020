package kr.co.green;


import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner input = new Scanner(System.in);
		int N = input.nextInt();
		input.close();
		int result = fiv(N);
		System.out.println(result);
		
	}
	
	public static int fiv(int n)
	{
		if(n==2)
		{
			return 1;
		}
		
		return fiv(n-1)+fiv(n-2);
	}
	
	
	
	

}

