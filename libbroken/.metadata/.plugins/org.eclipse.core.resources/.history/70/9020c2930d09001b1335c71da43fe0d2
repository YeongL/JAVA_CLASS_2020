package kr.co.green;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner input = new Scanner(System.in);
		int num = input.nextInt();
		while(true)
		{
			if(num == -1)
			{
				break;
			}
			if(num%3!=0)
			{
				continue;
			}
			else
			{
				System.out.println(num/3);
			}
		}
		
		
	}

}
