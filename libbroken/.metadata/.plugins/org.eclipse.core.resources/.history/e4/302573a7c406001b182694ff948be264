package kr.co.green;


import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner input = new Scanner(System.in);
		String A = input.next();
		String B = input.next();
		int numA = ((int)A.charAt(2)-'0')*100 + ((int)A.charAt(1)-'0')*10 +((int)A.charAt(0)-'0');
		int numB = ((int)B.charAt(2)-'0')*100 + ((int)B.charAt(1)-'0')*10 +((int)B.charAt(0)-'0');
		
		if(numA>numB)
		{
			System.out.println(numA);
		}
		else
		{
			System.out.println(numB);
		}
		
		
		
	
	}
	
	

}

