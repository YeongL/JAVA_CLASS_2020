package kr.co.green;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//Scanner input = new Scanner(System.in);
		for(int i=2;i<=4;i++)
		{
			for(int j=1;j<=5;j++)
			{
				System.out.printf("%d * %d =  %d   ",i,j,i*j);
			}
		}
		
	}

}
