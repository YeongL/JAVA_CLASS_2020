package kr.co.green;


import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner input = new Scanner(System.in);
		int num = input.nextInt();
		
		for(int i =1;i<=num;i++)
		{
			if(i%2 == 0)
			{
				System.out.println(i);
			}
			
		}
		
		
	}
	
	
	
	
	

}

