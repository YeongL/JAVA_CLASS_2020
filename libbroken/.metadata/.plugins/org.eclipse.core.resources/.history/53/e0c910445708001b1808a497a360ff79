package kr.co.green;


import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner input = new Scanner(System.in);
		int num = input.nextInt();
		input.close();
		int sum = 0;
		for(int i =num;i<=100;i++)
		{
			sum+=i;
			
		}
		System.out.println(sum);
		
	}
	
	
	
	
	

}

