package kr.co.green;


import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner input = new Scanner(System.in);
		
		int r = input.nextInt();
		
		System.out.println(r*r*Math.PI);
		System.out.println(2*r*r);
		
	}
	
	
	
	
	

}

